{
  "preset": "wordpress",
  "requireSpacesInsideParentheses": null,
  "requirePaddingNewLinesBeforeLineComments": false,
  "disallowSpacesInFunctionExpression": null,
  "disallowSpacesInAnonymousFunctionExpression": null,
  "requireVarDeclFirst": false,
  "requireSpaceAfterPrefixUnaryOperators": false,
  "requireYodaConditions": null,
  "requireCamelCaseOrUpperCaseIdentifiers": false,
  "requireCapitalizedComments": false,
  "fileExtensions": [".js"],
  "excludeFiles": [
    "js/dist/*.js",
    "js/src/legacy.js",
    "js/src/modules/lazyload.js",
    "js/src/modules/sticky-nav.js",
    "js/src/modules/navigation.js",
    "js/all.js",
    "gulpfile.js"
  ]
}
